class Solution(object):
    def findKthNumber(self, m, n, k):

        def countb(mid):
            count = 0
            for i in range(1, m + 1):
                count += min(mid // i, n)
            return count

        left = 1
        right = m*n

        while left<right:

            mid = (left+right)//2

            if countb(mid)>=k:

                right=mid

            else:

                left=mid+1

        return left

            
        

        
